{
  "manifest_version": 3,
  "name": "Meyer Utils",
  "version": "1.0",
  "permissions": ["contextMenus", "activeTab", "scripting", "clipboardWrite"],
  "background": {
      "service_worker": "background.js"
  },
  "web_accessible_resources": [
      {
          "resources": [
              "src/turndown.js",
              "src/showdown.min.js"
          ],
          "matches": ["<all_urls>"]
      }
  ]
}
